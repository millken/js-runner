{
  "name": "js-runner",
  "version": "0.2.4",
  "description": "Complete high-scaled reverse-proxy solution",
  "keywords": [
    "reverse",
    "proxy",
    "http",
    "http-proxy"
  ],
  "main": "./bin/js-runner",
  "bin": {
    "js-runner": "./bin/js-runner"
  },
  "directories": {
    "lib": "./lib"
  },
  "dependencies": {
    "read-installed": "0.2.2",
